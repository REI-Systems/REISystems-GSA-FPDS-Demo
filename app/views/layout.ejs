    <script src="/js/dependencies/jqwidgets/jqxdropdownlist.js"></script>
    <script src="/js/dependencies/jqwidgets/jqxmenu.js"></script>
    <script src="/js/dependencies/jqwidgets/jqxgrid.js"></script>
    <script src="/js/dependencies/jqwidgets/jqxgrid.pager.js"></script>
    <script src="/js/dependencies/jqwidgets/jqxgrid.sort.js"></script>
    <script src="/js/dependencies/jqwidgets/jqxgrid.filter.js"></script>
    <script src="/js/dependencies/jqwidgets/jqxgrid.storage.js"></script>
    <script src="/js/dependencies/jqwidgets/jqxgrid.columnsresize.js"></script>
    <script src="/js/dependencies/jqwidgets/jqxgrid.columnsreorder.js"></script>
    <script src="/js/dependencies/jqwidgets/jqxgrid.selection.js"></script>
    <script src="/js/dependencies/jqwidgets/jqxpanel.js"></script>
    <script src="/js/dependencies/jqwidgets/jqxcheckbox.js"></script>
    <script src="/js/dependencies/jqwidgets/jqxtabs.js"></script>
    <script src="/js/dependencies/jqwidgets/jqxlistbox.js"></script>
    <script src="/js/dependencies/jqwidgets/jqxangular.js"></script>
    <script src="/js/dependencies/angular-animate/angular-animate.js"></script>
    <script src="/js/dependencies/angular-aria/angular-aria.js"></script>
    <script src="/js/dependencies/angular-cookies/angular-cookies.js"></script>
    <script src="/js/dependencies/angular-messages/angular-messages.js"></script>
    <script src="/js/dependencies/angular-nvd3/angular-nvd3.js"></script>
    <script src="/js/dependencies/angular-resource/angular-resource.js"></script>
    <script src="/js/dependencies/angular-route/angular-route.js"></script>
    <script src="/js/dependencies/angular-sanitize/angular-sanitize.js"></script>
    <script src="/js/dependencies/angular-touch/angular-touch.js"></script>
    <script src="/js/dependencies/bootstrap/bootstrap.js"></script>
    <script src="/js/dependencies/d3/d3.js"></script>
    <script src="/js/dependencies/metisMenu/metisMenu.js"></script>
    <script src="/js/dependencies/mocha/mocha.js"></script>
    <script src="/js/dependencies/moment/moment.js"></script>
    <script src="/js/dependencies/morrisjs/morris.js"></script>
    <script src="/js/dependencies/nvd3/nv.d3.js"></script>
