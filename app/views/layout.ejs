    <script src="/js/dependencies/sb-admin-2.js"></script>
    <script src="/js/dependencies/spin.js"></script>
    <script src="/js/dependencies/x2js/xml2json.min.js"></script>
    <script src="/js/Common/CommonController.js"></script>
